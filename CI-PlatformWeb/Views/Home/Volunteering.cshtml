                        <div class="rule d-flex mt-4">
                            <hr style="border:1px solid black">
                            <button class="date-btn mt-1">From @Model.StartDateEndDate.ToString().Split(" ")[0]</button>
                            <hr style="border:1px solid black">

                        </div>
                    }
                    else
                    {
                        <div class="rule d-flex mt-4">
                            <hr style="border:1px solid black">
                            <button class="date-btn mt-1">@Model.GoalText</button>
                            <hr style="border:1px solid black">

                        </div>
                    }
                    <div class="row landing-page-box-border">

                        <div class="col-6 col-md-6 d-flex align-items-center">
                            <p class="lead me-2 pt-2">
                                <img src="~/images/Seats-left.png" alt="">
                                <div>
                                    <div>10</div>
                                    <div class="landing-page-card-vol-small">Seats Left</div>
                                </div>
                            </p>
                        </div>
                        <div class="col-6 col-md-6 d-flex align-items-center">
                            @if (Model.missionType == "time")
                            {
                                <div class="col-6 col-md-6 d-flex align-items-center">
                                    <p class="lead me-2 pt-2">
                                        <img src="~/images/deadline.png" alt="">
                                        <div>
                                            <div>@Model.EndDate.ToString().Split(" ")[0]</div>
                                            <div class="landing-page-card-vol-small">Deadline</div>
                                        </div>
                                    </p>
                                </div>
                            }
                            else
                            {
                                <p class="lead me-2 pt-2">
                                    <img src="~/images/achieved.png" alt="">
                                    <div style="width: 100%;">
                                        <div>

                                            <div class="progress" role="progressbar" aria-label="Success example" aria-valuenow="50"
                                             aria-valuemin="0" aria-valuemax="100" style="height: 10px;">
                                                <div class="progress-bar bg-success" style="width: 25%;"></div>
                                            </div>

                                        </div>
                                        <div class="landing-page-card-vol-small">8000 achived</div>
                                    </div>
                                </p>
                            }
                        </div>
                    </div>
                   
                    <div class="row">
                        <div class="col  col-12  col-xl-5 col-lg-12 col-md-5 col-sm-12  me-xl-4 me-md-4 rounded-5 d-flex mt-3  Addfavourite   justify-content-center" id="favmissiondiv">
                        @{
                        var useridforFavM = Context.Session.GetInt32("userIDforfavmission");
                        }
                        @if(Model.UserId == useridforFavM && Model.isFavrouite != null)
                        {
                        <button class="btn vol-page-group-btn-btn" id="afterApply" onclick="addToFav(@Model.MissionId,@useridforFavM)">
                        <i class="bi bi-heart-fill" id="heart"></i> Already Added
                        </button>
                        }
                        else
                        {
                        <button class="btn vol-page-group-btn-btn" id="beforeApply" onclick="addToFav(@Model.MissionId,@useridforFavM)">
                        <i class="bi bi-heart" id="heart"></i> Add
                        to Favourite
                        </button>
                        }

                        </div>
                         
                        <div class="col col-12 col-xl-6 col-lg-12  col-md-6 col-sm-12 rounded-5 d-flex  mt-3 Addfavourite  justify-content-center">
                            <div><img src="~/images/add1.png" /></div>

                            <label class="ms-2"> Recommend to a Co-Worker</label>
                        </div>

                    </div>
                    <div class="rule d-flex mt-4">
                        <hr style="border:1px solid black">

                        @{
                            var useridforrating = Context.Session.GetInt32("userIDforfavmission");
                        }

                        @if (@Model.Rating != null)
                        {

                            int count = 0;

                            @for (var i = 1; i <= @Model.Rating; i++)
                            {
                                <i class="bi bi-star" id="@i" onclick="addRating(this.id,@Model.MissionId,@useridforrating)"></i>
                                count++;
                            }
                            @for (var k = count + 1; k <= 5; k++)
                            {
                                <i class="bi bi-star" id="@k" onclick="addRating(this.id,@Model.MissionId,@useridforrating)"></i>
                            }

                        }
                        else
                        {
                            <i class="bi bi-star" id="1" onclick="addRating(this.id,@Model.MissionId,@useridforrating)"></i>
                            <i class="bi bi-star" id="2" onclick="addRating(this.id,@Model.MissionId,@useridforrating)"></i>
                            <i class="bi bi-star" id="3" onclick="addRating(this.id,@Model.MissionId,@useridforrating)"></i>
                            <i class="bi bi-star" id="4" onclick="addRating(this.id,@Model.MissionId,@useridforrating)"></i>
                            <i class="bi bi-star" id="5" onclick="addRating(this.id,@Model.MissionId,@useridforrating)"></i>
                        }

                        <hr style="border:1px solid black">

                    </div>
